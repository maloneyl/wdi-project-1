.row
  .col-md-8.col-md-offset-1

    %h2 Login

    = form_tag sessions_path do
      .field
        = label_tag :email
        %br/
        = email_field_tag :email
      .field
        = label_tag :password
        %br/
        = password_field_tag :password
        %br/
        %br/
      .actions
        = submit_tag "Login!"
